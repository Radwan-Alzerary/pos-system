  $(document).on("click", "[id^=page]", function () {
    var id = this.id;
    const pagenum = id.replace("page", "");
    url = window.location.href;
    if (url.includes("page")) {
      url = url.replace(/page=[^&]+/, `page=${pagenum}`);
    } else if (!url.includes("?")) {
      url += `?page=${pagenum}`;
    } else {
      url += `&page=${pagenum}`;
    }
